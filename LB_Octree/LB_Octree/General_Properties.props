    <BuildMacro Include="PCL_DIR">
      <Value>$(PCL_DIR)</Value>
    </BuildMacro>
    <BuildMacro Include="LIBS_DIR">
      <Value>$(LIBS_DIR)</Value>
    </BuildMacro>
    <BuildMacro Include="BOOST_DIR">
      <Value>$(BOOST_DIR)</Value>
    </BuildMacro>
  </ItemGroup>
</Project>